<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
 	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 	xsi:schemaLocation="http://www.springframework.org/schema/beans
  		http://www.springframework.org/schema/beans/spring-beans.xsd">
  		
  		<bean class="register.controller.RegisterController">
			<property name="sendMail" ref="sendMail"/>
			<property name="memberRegisterService" ref="memberRegSvc"/>
  		</bean>
  		
  		<bean class="register.controller.LoginController">
  			<property name="authService" ref="authService"/>
  		</bean>
  		
  		<bean class="register.controller.MainController"/>
  		
</beans>